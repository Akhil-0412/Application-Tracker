{
    "name": "application-tracker",
    "rewrites": [
        {
            "source": "/static/(.*)",
            "destination": "/dashboard/static/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.py"
        }
    ],
    "crons": [
        {
            "path": "/api/process",
            "schedule": "0 * * * *"
        }
    ]
}